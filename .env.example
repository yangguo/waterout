# ── Generic (used by all backends; also checked as fallback) ───────────────
ENHANCE_API_KEY=your-api-key-here

# ── Backend-specific keys (use whichever backend you choose) ──────────────
# ARK_API_KEY=your-ark-api-key-here        # Ark / Volcengine
# GEMINI_API_KEY=your-gemini-api-key-here  # Google Gemini
# XAI_API_KEY=your-xai-api-key-here        # xAI (Grok)
# HUNYUAN_SECRET_ID=your-secret-id-here    # Tencent Hunyuan (native SDK)
# HUNYUAN_SECRET_KEY=your-secret-key-here  # Tencent Hunyuan (native SDK)

# ── Base URL: auto-detected from model name; override only if needed ───────
# ENHANCE_BASE_URL=https://api-inference.modelscope.cn/v1
# ENHANCE_BASE_URL=https://ark.cn-beijing.volces.com/api/v3
# ENHANCE_BASE_URL=https://generativelanguage.googleapis.com/v1beta
# ENHANCE_BASE_URL=https://api.hunyuan.cloud.tencent.com/v1
# ENHANCE_BASE_URL=https://api.x.ai/v1

# ── Model: auto-selected per backend; override to pin a specific version ───
# ENHANCE_MODEL=Qwen/Qwen-Image-Edit-2511
# ENHANCE_MODEL=doubao-seedream-4-5-251128
# ENHANCE_MODEL=gemini-2.5-flash-image
# ENHANCE_MODEL=hunyuan-image-3.0-instruct
# ENHANCE_MODEL=grok-imagine-image

# ── Output size (not used by Gemini) ──────────────────────────────────────
# ENHANCE_SIZE=1024x1536   # ModelScope
# ENHANCE_SIZE=4K          # Ark
# ENHANCE_SIZE=2k          # xAI

# ── Custom prompt (falls back to built-in default if unset) ───────────────
# ENHANCE_PROMPT=
